{
  "name": "gary-eisen-project-up",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "jest --watchAll --coverage --collectCoverageFrom='[\"src/**/*.{js,jsx}\", \"src/**/*.page.{js,jsx}\" ,\"!src/**/*.test.{js,jsx}\"]' --coveragePathIgnorePatterns='[\"node_modules\", \"coverage\", \"build\"]'"
  },
  "dependencies": {
    "aos": "^3.0.0-beta.6",
    "daisyui": "^2.51.6",
    "firebase": "^9.22.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-firebase-hooks": "^5.1.1",
    "react-ga4": "^2.1.0",
    "react-hook-form": "^7.44.2",
    "react-icons": "^4.8.0",
    "react-router-dom": "^6.11.2",
    "react-toastify": "^9.1.3",
    "recharts": "^2.6.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "@testing-library/jest-dom": "^6.1.6",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^23.0.1",
    "jsdom-global": "^3.0.2",
    "postcss": "^8.4.23",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^29.1.1",
    "vite": "^4.3.2"
  },
  "jest": {
    "moduleNameMapper": {
      "^react/jsx-runtime$": "react/jsx-runtime"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testEnvironment": "jest-environment-jsdom",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}
